{
    "name": "appolodev/sf-ui-toolbox",
    "version": "0.0.20",
    "description": "Toolbox UI for Symfony",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Fredxd",
            "email": "frederic@appolo.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "ext-intl": "*",
        "ext-zip": "*",
        "php": ">=8.2",
        "twig/twig": "^3.0",
        "twig/html-extra": "^3.12",
        "tales-from-a-dev/twig-tailwind-extra": "^1.0",
        "symfony/ux-twig-component": "^2.9",
        "symfony/twig-bundle": "^8.0",
        "symfony/ux-icons": "^2.18"
    },
    "require-dev": {
        "symfony/maker-bundle": "^1.49",
        "friendsofphp/php-cs-fixer": "^3.22",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AppoloDev\\SFUIToolboxBundle\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    }
}
